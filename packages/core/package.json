{
  "name": "@svge/core",
  "description": "Transform SVG into Vue Components.",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "keywords": [],
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "scripts": {
    "reset": "rm -rf dist",
    "build": "pnpm reset && tsup src/index.ts --dts --format esm,cjs"
  },
  "peerDependencies": {
    "@svge/plugin-vue": "workspace:^0.0.1"
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "cosmiconfig": "^7.0.1"
  }
}
